<h1>New Schedule</h1>
<%= form_for @schedule, url: {action: "create",controller: "schedules"} do |f| %>
	<p>
		<%=f.label :title %><br />
		<%=f.text_field :title %><br />
	</p>
	<p>
		<%=f.label :category %><br />
		<%=f.text_field :category %><br />
	</p>
	<p>
		<%=f.label :year %><br />
		<%=f.text_field :year %><br />
	</p>
	<p>
		<%=f.label :month %><br />
		<%=f.text_field :month %><br />
	</p>
	<p>
		<%=f.label :day %><br />
		<%=f.text_field :day %><br />
	</p>
	<p>
		<%=f.label :star %><br />
		<%=f.text_field :star %><br />
	</p>
	<p>
		<%=f.label :note %><br />
		<%=f.text_field :note %><br />
	</p>
	<p>
		<%=f.label :remind_date %><br />
		<%=f.text_field :remind_date %><br />
	</p>
		<p>
		<%=f.label :remind_time %><br />
		<%=f.text_field :remind_time %><br />
	</p>
		<p>
		<%=f.label :ring %><br />
		<%=f.text_field :ring %><br />
	</p>
		<p>
		<%=f.label :submit %><br />
		<%=f.text_field :submit %><br />
	</p>
	<p>
		<%=f.label :accept %><br />
		<%=f.text_field :accept %><br />
	</p>
	<p>
		<%=f.label :image %><br />
		<%=f.text_field :image %><br />
	</p>
	<p>
		<%=f.text_field :userID, :value => params[:userID] %><br />
	</p>
	<p>
		<%= f.submit "Add Schedule" %>
	</p>
<% end %>
<%= link_to "Go Back", schedules_index_path %>
